# Environment defaults for yourwebsquad-boilerplate
# Copy to .env and adjust per environment.

# deployment environment: development | staging | production
DEPLOY_ENV=development

# canonical URLs
SITE_URL=https://example.com
STAGING_URL=https://staging.example.com

# comma-separated origins allowed for forms integration
ALLOW_ORIGINS=example.com,staging.example.com

# set to 1 to disallow indexing (robots.txt)
DISALLOW_ROBOTS=0
